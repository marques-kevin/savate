{
  "name": "site",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "amplitude-js": "https://github.com/amplitude/Amplitude-Javascript.git",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "body-scroll-lock": "^2.5.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "connected-react-router": "^4.4.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "firebase": "5.3.0",
    "fs-extra": "3.0.1",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "2.29.0",
    "intl": "^1.2.5",
    "jest": "20.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.2.0",
    "raven-js": "^3.22.1",
    "react": "^16.5.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.5.1",
    "react-intl": "^2.4.0",
    "react-intl-redux": "^0.7.0",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reset.css": "^2.0.2",
    "style-loader": "0.19.0",
    "styled-components": "^3.4.6",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "open-coverage": "open coverage/lcov-report/index.html",
    "preinstall": "node scripts/preinstall.js",
    "start": "node scripts/start.js",
    "start:production": "NODE_ENV=production node scripts/start.js",
    "build":
      "node scripts/build.js && npm run gzip:sw && npm run load-css && npm run gzip:html",
    "build:surge": "SURGE=true node scripts/build.js && npm run load-css",
    "deploy:surge": "surge ./build books-jsjitsu.surge.sh",
    "test": "node scripts/test.js --env=jsdom",
    "test:ci": "CI=true node scripts/test.js --env=jsdom --coverage",
    "coverage": "node scripts/test.js --env=jsdom --coverage",
    "precommit": "npm run test:ci && lint-staged",
    "commit": "git-cz",
    "do:release": "corp-semantic-release",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "release":
      "npm run test:ci && npm run do:release && npm run build && npm run deploy:ci",
    "gzip:sw":
      "gzip build/service-worker.js && mv build/service-worker.js.gz build/service-worker.js",
    "gzip:html":
      "gzip build/index.html && mv build/index.html.gz build/index.html",
    "load-css": "node scripts/load-css.js",
    "deploy:production": "node scripts/deploy.js --stage production",
    "deploy:ci": "CI=true npm run deploy:production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {}
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss}": ["prettier --write", "git add"]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/stores/**/*",
      "!src/index.js",
      "!src/config/*",
      "!src/utils/sentry.js",
      "!src/utils/amplitude.js",
      "!src/routes/index.js",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.{js,jsx,mjs}",
      "<rootDir>/functions/**/__tests__/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)":
        "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(scss|css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.js",
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^6.1.3",
    "@storybook/react": "^3.3.15",
    "babel-plugin-emotion": "^9.2.9",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "commitizen": "^2.9.6",
    "commitlint": "^6.1.3",
    "compression-webpack-plugin": "^1.1.3",
    "corp-semantic-release": "^6.4.0",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "fetch-mock": "^6.0.1",
    "ghooks": "^2.0.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^6.0.1",
    "node-sass": "^4.13.1",
    "preload-webpack-plugin": "^2.1.1",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "surge": "^0.20.1",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  }
}
